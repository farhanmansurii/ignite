{
  "name": "@farhanmansuri/ignite-core",
  "version": "1.0.5",
  "description": "Serverless cold-start elimination \u2014 framework-agnostic signal logic",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farhanmansurii/ignite.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/farhanmansurii/ignite/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/farhanmansurii/ignite/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run"
  },
  "keywords": [
    "serverless",
    "cold-start",
    "pre-warming",
    "performance",
    "firebase",
    "aws-lambda",
    "cloudflare-workers",
    "edge",
    "optimization"
  ],
  "devDependencies": {
    "happy-dom": "^14.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}