{
  "name": "@farhanmansuri/ignite-core",
  "version": "1.0.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "keywords": [
    "serverless",
    "cold-start",
    "pre-warming",
    "performance",
    "firebase",
    "aws-lambda",
    "cloudflare-workers",
    "edge",
    "optimization"
  ],
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
