{
  "name": "@farhanmansuri/ignite-firebase",
  "version": "1.0.5",
  "description": "Firebase Functions middleware for Ignite \u2014 early-exit on warm signals",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farhanmansurii/ignite.git",
    "directory": "packages/firebase"
  },
  "homepage": "https://github.com/farhanmansurii/ignite/tree/master/packages/firebase#readme",
  "bugs": {
    "url": "https://github.com/farhanmansurii/ignite/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "firebase-functions": ">=4.0.0"
  },
  "keywords": [
    "firebase",
    "firebase-functions",
    "serverless",
    "cold-start",
    "pre-warming",
    "middleware",
    "cloud-functions",
    "performance",
    "optimization"
  ],
  "devDependencies": {
    "@types/express": "^5.0.6",
    "firebase-functions": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}